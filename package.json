{
  "name": "@jhqn/eslint-config-monorepo",
  "version": "0.3.0",
  "private": true,
  "description": "jhqn's ESLint configs",
  "author": "jade-gjz",
  "license": "MIT",
  "keywords": [],
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "update": "pnpm up -i --latest -r",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public"
  },
  "devDependencies": {
    "@jhqn/eslint-config-ts": "workspace:*",
    "bumpp": "^8.0.0",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.31.0",
    "typescript": "^4.9.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "eslint"
      ]
    }
  }
}

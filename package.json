{
  "name": "@jhqn/eslint-config",
  "type": "module",
  "version": "0.6.14",
  "packageManager": "pnpm@8.15.4",
  "description": "jhqn's ESLint config",
  "author": "jade-gjz",
  "license": "MIT",
  "homepage": "https://github.com/jaderd-jh/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaderd-jh/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/jaderd-jh/eslint-config/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.js",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm",
    "dev": "tsup --format esm,cjs --watch & eslint-flat-config-viewer",
    "watch": "tsup --format esm,cjs --watch",
    "lint": "eslint .",
    "prepack": "pnpm build",
    "updeps": "pnpm up -i --latest -r",
    "release": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@unocss/eslint-plugin": ">=0.50.0",
    "astro-eslint-parser": "^0.16.3",
    "eslint": ">=8.40.0",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-svelte": "^2.34.1",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-slidev": "^1.0.5",
    "svelte-eslint-parser": "^0.33.1"
  },
  "peerDependenciesMeta": {
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "prettier-plugin-astro": {
      "optional": true
    },
    "prettier-plugin-slidev": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/eslint-define-config": "^1.23.0-2",
    "@antfu/install-pkg": "^0.3.1",
    "@eslint-types/jsdoc": "^48.2.0",
    "@eslint-types/typescript-eslint": "^7.0.2",
    "@eslint-types/unicorn": "^51.0.1",
    "@stylistic/eslint-plugin": "^1.7.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint-config-flat-gitignore": "^0.1.3",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-antfu": "^2.1.2",
    "eslint-plugin-ecmascript-compat": "^3.2.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-jsonc": "^2.13.0",
    "eslint-plugin-markdown": "^4.0.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-perfectionist": "^2.7.0",
    "eslint-plugin-toml": "^0.9.2",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-vitest": "^0.3.25",
    "eslint-plugin-vue": "^9.23.0",
    "eslint-plugin-yml": "^1.12.2",
    "eslint-processor-vue-blocks": "^0.1.1",
    "globals": "^14.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "parse-gitignore": "^2.0.0",
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2",
    "toml-eslint-parser": "^0.9.3",
    "vue-eslint-parser": "^9.4.2",
    "yaml-eslint-parser": "^1.2.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@antfu/eslint-plugin-prettier": "^5.0.1-1",
    "@jhqn/eslint-config": "workspace:*",
    "@stylistic/eslint-plugin-migrate": "^1.7.0",
    "@types/eslint": "^8.56.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.27",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.32",
    "@unocss/eslint-plugin": "^0.58.6",
    "astro-eslint-parser": "^0.16.3",
    "bumpp": "^9.4.0",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "npm:eslint-ts-patch@^8.57.0-0",
    "eslint-flat-config-viewer": "^0.1.11",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-svelte": "^2.35.1",
    "eslint-ts-patch": "^8.57.0-0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "nano-staged": "^0.8.0",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-slidev": "^1.0.5",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.10.0",
    "svelte": "^4.2.12",
    "svelte-eslint-parser": "^0.33.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2",
    "vitest": "^1.3.1",
    "vue": "^3.4.21"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "eslint"
      ]
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged"
  },
  "nano-staged": {
    "*": "eslint --cache --quiet --fix"
  }
}

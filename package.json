{
  "name": "@jhqn/eslint-config-monorepo",
  "version": "0.6.3",
  "private": true,
  "description": "jhqn's ESLint configs",
  "author": "jade-gjz",
  "packageManager": "pnpm@8.10.3",
  "scripts": {
    "lint": "eslint .",
    "update": "pnpm up -i --latest -r",
    "release": "bumpp -r"
  },
  "devDependencies": {
    "@jhqn/eslint-config-ts": "workspace:*",
    "bumpp": "^9.2.0",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.51.0",
    "typescript": "^5.2.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "eslint"
      ]
    }
  }
}

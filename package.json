{
  "name": "@jhqn/eslint-config-monorepo",
  "version": "0.6.0",
  "private": true,
  "description": "jhqn's ESLint configs",
  "author": "jade-gjz",
  "packageManager": "pnpm@8.6.12",
  "scripts": {
    "preinstall": "npm i -g pnpm",
    "lint": "eslint .",
    "update": "pnpm up -i --latest -r",
    "release": "bumpp -r"
  },
  "devDependencies": {
    "@jhqn/eslint-config-ts": "workspace:*",
    "bumpp": "^9.1.1",
    "conventional-changelog-cli": "^3.0.0",
    "eslint": "^8.46.0",
    "typescript": "^5.1.6"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "eslint"
      ]
    }
  }
}

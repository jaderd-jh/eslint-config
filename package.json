{
  "name": "@jhqn/eslint-config",
  "type": "module",
  "version": "0.7.13",
  "packageManager": "pnpm@9.6.0",
  "description": "jhqn's ESLint config",
  "author": "jade-gjz",
  "license": "MIT",
  "homepage": "https://github.com/jaderd-jh/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaderd-jh/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/jaderd-jh/eslint-config/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.js",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "nr typegen && tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm",
    "dev": "tsup --format esm,cjs --watch & npx @eslint/config-inspector",
    "build:inspector": "pnpm build && npx @eslint/config-inspector build",
    "watch": "tsup --format esm,cjs --watch",
    "lint": "eslint .",
    "fix:lint": "eslint --fix .",
    "typegen": "esno scripts/typegen.ts",
    "prepack": "pnpm build",
    "updeps": "pnpm up -i --latest -r",
    "release": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^1.5.14",
    "@unocss/eslint-plugin": ">=0.50.0",
    "astro-eslint-parser": "^1.0.2",
    "eslint": ">=8.40.0",
    "eslint-plugin-astro": "^1.2.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-solid": "^0.14.0",
    "eslint-plugin-svelte": "^2.39.0",
    "prettier-plugin-astro": "^0.14.0",
    "prettier-plugin-slidev": "^1.0.5",
    "svelte-eslint-parser": "^0.41.0"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-solid": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "prettier-plugin-astro": {
      "optional": true
    },
    "prettier-plugin-slidev": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^0.3.3",
    "@clack/prompts": "^0.7.0",
    "@stylistic/eslint-plugin": "^2.6.0-beta.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0-alpha.51",
    "@typescript-eslint/parser": "^8.0.0-alpha.51",
    "eslint-config-flat-gitignore": "^0.1.8",
    "eslint-flat-config-utils": "^0.3.0",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-antfu": "^2.3.4",
    "eslint-plugin-command": "^0.2.3",
    "eslint-plugin-ecmascript-compat": "^3.2.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^3.1.0",
    "eslint-plugin-jsdoc": "^48.8.3",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-perfectionist": "^3.0.0",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-toml": "^0.11.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-unused-imports": "^4.0.1",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "^9.27.0",
    "eslint-plugin-yml": "^1.14.0",
    "eslint-processor-vue-blocks": "^0.1.2",
    "globals": "^15.8.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "parse-gitignore": "^2.0.0",
    "picocolors": "^1.0.1",
    "toml-eslint-parser": "^0.10.0",
    "vue-eslint-parser": "^9.4.3",
    "yaml-eslint-parser": "^1.2.3",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@antfu/ni": "^0.22.0",
    "@eslint-react/eslint-plugin": "^1.5.30",
    "@eslint/config-inspector": "^0.5.2",
    "@jhqn/eslint-config": "workspace:*",
    "@stylistic/eslint-plugin-migrate": "^2.3.0",
    "@types/eslint": "^9.6.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.12",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.32",
    "@unocss/eslint-plugin": "^0.61.5",
    "astro-eslint-parser": "^1.0.2",
    "bumpp": "^9.4.1",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "npm:eslint-ts-patch@9.5.0-0",
    "eslint-plugin-astro": "^1.2.3",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-solid": "^0.14.1",
    "eslint-plugin-svelte": "^2.43.0",
    "eslint-ts-patch": "9.5.0-0",
    "eslint-typegen": "^0.3.0",
    "esno": "^4.7.0",
    "execa": "^9.3.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "nano-staged": "^0.8.0",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-slidev": "^1.0.5",
    "rimraf": "^6.0.0",
    "simple-git-hooks": "^2.11.1",
    "svelte": "^4.2.18",
    "svelte-eslint-parser": "^0.41.0",
    "tsup": "^8.2.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vitest": "^2.0.4",
    "vue": "^3.4.34"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "eslint"
      ]
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged"
  },
  "nano-staged": {
    "*": "eslint --cache --quiet --fix"
  },
  "$schema": "https://json.schemastore.org/package.json"
}

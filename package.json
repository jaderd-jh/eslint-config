{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@jhqn/eslint-config",
  "type": "module",
  "version": "0.11.1",
  "packageManager": "pnpm@10.28.2",
  "description": "jhqn's ESLint config",
  "author": "jade-gjz",
  "license": "MIT",
  "homepage": "https://github.com/jaderd-jh/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaderd-jh/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/jaderd-jh/eslint-config/issues"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./dist/cli.mjs",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.mts",
  "bin": "./bin/index.mjs",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "nr typegen && tsdown --clean --dts",
    "stub": "tsdown",
    "dev": "tsdown --format esm,cjs --watch & npx @eslint/config-inspector",
    "build:inspector": "pnpm build && npx @eslint/config-inspector build",
    "watch": "tsdown --watch",
    "lint": "eslint",
    "fix:lint": "eslint --fix",
    "typegen": "tsx scripts/typegen.ts",
    "prepack": "pnpm build",
    "updeps": "pnpm up -i --latest -r",
    "release": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^2.11.0",
    "@unocss/eslint-plugin": "^66.6.0",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.39.2 || ^10.0.0",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-jsx-a11y": ">=6.10.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-svelte": "^3.14.0",
    "eslint-plugin-vue": "^10.7.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "eslint-processor-vue-blocks": "^2.0.0",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-slidev": "^1.0.5",
    "svelte-eslint-parser": "^1.4.1",
    "vue-eslint-parser": "^10.2.0"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-solid": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "eslint-plugin-vuejs-accessibility": {
      "optional": true
    },
    "eslint-processor-vue-blocks": {
      "optional": true
    },
    "prettier-plugin-astro": {
      "optional": true
    },
    "prettier-plugin-slidev": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.1.0",
    "@clack/prompts": "^1.0.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.6.0",
    "@eslint/markdown": "^7.5.1",
    "@stylistic/eslint-plugin": "^5.9.0",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "@vitest/eslint-plugin": "^1.6.9",
    "ansis": "^4.2.0",
    "cac": "^6.7.14",
    "eslint-config-flat-gitignore": "^2.2.1",
    "eslint-flat-config-utils": "^3.0.1",
    "eslint-merge-processors": "^2.0.0",
    "eslint-plugin-antfu": "^3.2.2",
    "eslint-plugin-command": "^3.4.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsdoc": "^62.7.0",
    "eslint-plugin-jsonc": "^3.1.0",
    "eslint-plugin-n": "^17.24.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^5.6.0",
    "eslint-plugin-regexp": "^3.0.0",
    "eslint-plugin-toml": "^1.3.0",
    "eslint-plugin-unicorn": "^63.0.0",
    "eslint-plugin-unused-imports": "^4.4.1",
    "eslint-plugin-yml": "^3.3.0",
    "globals": "^17.3.0",
    "jsonc-eslint-parser": "^3.1.0",
    "local-pkg": "^1.1.2",
    "parse-gitignore": "^2.0.0",
    "toml-eslint-parser": "^1.0.3",
    "yaml-eslint-parser": "^2.0.0"
  },
  "devDependencies": {
    "@antfu/ni": "^28.2.0",
    "@eslint-react/eslint-plugin": "^2.13.0",
    "@eslint/config-inspector": "^1.4.2",
    "@jhqn/eslint-config": "workspace:*",
    "@stylistic/eslint-plugin-migrate": "^4.4.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/node": "^24.10.9",
    "@unocss/eslint-plugin": "^66.6.0",
    "astro-eslint-parser": "^1.3.0",
    "bumpp": "^10.4.1",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-astro": "^1.6.0",
    "eslint-plugin-erasable-syntax-only": "^0.4.0",
    "eslint-plugin-format": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.2",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-svelte": "^3.15.0",
    "eslint-plugin-vue": "^10.8.0",
    "eslint-plugin-vuejs-accessibility": "^2.5.0",
    "eslint-processor-vue-blocks": "^2.0.0",
    "eslint-typegen": "^2.3.1",
    "execa": "^9.6.1",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-slidev": "^1.0.5",
    "simple-git-hooks": "^2.13.1",
    "svelte": "^5.53.3",
    "svelte-eslint-parser": "^1.5.0",
    "tinyglobby": "^0.2.15",
    "tsdown": "^0.20.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "vue": "^3.5.28",
    "vue-eslint-parser": "^10.4.0"
  },
  "pnpm": {
    "overrides": {
      "is-core-module": "npm:@nolyfill/is-core-module@^1",
      "safe-buffer": "npm:@nolyfill/safe-buffer@^1",
      "safer-buffer": "npm:@nolyfill/safer-buffer@^1",
      "side-channel": "npm:@nolyfill/side-channel@^1"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "simple-git-hooks",
      "unrs-resolver"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}

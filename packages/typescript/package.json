{
  "name": "@jhqn/eslint-config-ts",
  "version": "0.2.2",
  "description": "jhqn's ESLint configuration for TypeScript",
  "author": "jade-gjz",
  "license": "MIT",
  "keywords": [
    "jhqn",
    "eslint",
    "eslint-config"
  ],
  "main": "index.js",
  "files": [
    "*.js"
  ],
  "scripts": {
    "lint": "eslint . --config=index.js",
    "postpublish": "conventional-changelog -p angular -i CHANGELOG.md -s -r 2 && git add CHANGELOG.md && git commit -am \"chore(docs): update CHANGELOG.md\" && git push"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@jhqn/eslint-config-basic": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0"
  }
}

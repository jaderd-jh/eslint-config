{
  "name": "@jhqn/eslint-config-solid",
  "version": "0.2.2",
  "description": "jhqn's eslint config for solid",
  "author": "jade-gjz",
  "license": "MIT",
  "keywords": [
    "jhqn",
    "eslint",
    "eslint-config"
  ],
  "main": "index.js",
  "files": [
    "*.js"
  ],
  "scripts": {
    "lint": "eslint . --config=index.js",
    "postpublish": "conventional-changelog -p angular -i CHANGELOG.md -s -r 2 && git add CHANGELOG.md && git commit -am \"chore(docs): update CHANGELOG.md\" && git push"
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  },
  "dependencies": {
    "@jhqn/eslint-config-ts": "workspace:*",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-solid": "^0.9.1"
  }
}

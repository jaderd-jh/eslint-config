{
  "name": "@jhqn/eslint-config-basic",
  "version": "0.3.0",
  "description": "jhqn's basic eslint config",
  "author": "jade-gjz",
  "license": "MIT",
  "keywords": [
    "jhqn",
    "eslint",
    "eslint-config"
  ],
  "main": "index.js",
  "files": [
    "*.js"
  ],
  "scripts": {
    "lint": "eslint . --config=index.js",
    "postpublish": "conventional-changelog -p angular -i CHANGELOG.md -s -r 2 && git add CHANGELOG.md && git commit -am \"chore(docs): update CHANGELOG.md\" && git push"
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-ecmascript-compat": "^2.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-jsonc": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-regexp": "^1.11.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "eslint-plugin-yml": "^1.4.0",
    "jsonc-eslint-parser": "^2.1.0",
    "prettier": "^2.8.1",
    "yaml-eslint-parser": "^1.1.0"
  }
}
